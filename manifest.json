{
    "manifest_version": 3,
    "name": "Tab Manager for Chrome",
    "version": "1.0",
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/group.svg"
    },
    "permissions": [
        "tabs",
        "storage",
        "tabGroups"
    ],
    "action": {
        "default_popup": "tab.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}