{
    "manifest_version": 3,
    "name": "Tab Manager for Chrome",
    "version": "1.0",
    "icons": {
      "16": "images/tab.png",
      "32": "images/tab.png",
      "48": "images/tab.png",
      "128": "images/tab.png"
    },
    "permissions": [
        "tabs",
        "tabGroups"
    ],
    "action": {
        "default_popup": "tab.html"
    }
}